datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id            String    @id @default(cuid())
  name          String?
  email         String    @unique
  password      String
  analyses      Analysis[]
  createdAt     DateTime  @default(now())
  updatedAt     DateTime  @updatedAt
}

model Analysis {
  id          String    @id @default(cuid())
  title       String
  data        Json
  summary     Json
  charts      Json
  userId      String
  user        User      @relation(fields: [userId], references: [id])
  createdAt   DateTime  @default(now())
  updatedAt   DateTime  @updatedAt
}